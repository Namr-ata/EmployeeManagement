@{
}
@model List<Employee>

<table class="table">
    <tr>
        <th>Name</th>
        <th>Address</th>
        <th>Salary</th>
        <th>Department</th>
        <th>Action</th>
    </tr>
    @foreach (var employee in Model)
    {
        <tr>
            <td>@employee.Name</td>
            <td>@employee.Address</td>
            <td>@employee.Salary</td>
            <td>@employee.Department</td>
            <td>
                <a href="@Url.Action("Edit", new { id = employee.Id })">Edit</a> |
                <a href="@Url.Action("Delete", new { id = employee.Id })">Delete</a>
            </td>
        </tr>
    }
</table>
